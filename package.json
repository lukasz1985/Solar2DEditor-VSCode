{
	"name": "solar2d-autocomplete",
	"displayName": "Solar2D Autocomplete",
	"description": "Autocomplete/Snippets for Solar2D Apis",
	"publisher": "solar2D",
	"version": "1.1.2",
	"icon": "Solar2D.png",
	"repository":"https://github.com/solar2d/Solar2D-VSAutocomplete",
	"engines": {
		"vscode": "^1.68.0"
	},
	"categories": [
		"Snippets",
		"Debuggers"
	],
	"keywords": [
		"solar2d",
		"solar2D",
		"corona",
		"coronaSDK",
		"lua",
		"docs",
		"documentation"
	],
	"activationEvents": [],
	"main": "./extension.js",
	"contributes": {
		"snippets": [{
			"language": "lua",
			"path": "./snippets/snippets.code-snippets"
		}],
		"commands": [
			{
				"command": "extension.solar2D.launchProj",
				"title": "Solar2D: Launch Project"
			},
			{
				"command": "extension.solar2D.launchProjConsole",
				"title": "Solar2D: Launch Project (With Console)"
			},
			{
				"command": "extension.solar2D.searchDocs",
				"title": "Solar2D: Search Docs"
			}

		]
	}
}
